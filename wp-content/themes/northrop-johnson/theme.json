{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "useRootPaddingAwareAlignments": true,
    "color": {
      "defaultPalette": false,
      "defaultGradients": false,
      "palette": [
        {
          "slug": "primary",
          "color": "#002033",
          "name": "Primary"
        },
        {
          "slug": "secondary",
          "color": "#1a3a4a",
          "name": "Secondary"
        },
        {
          "slug": "accent",
          "color": "#ff6900",
          "name": "Accent"
        },
        {
          "slug": "base",
          "color": "#faf9f7",
          "name": "Base (Light)"
        },
        {
          "slug": "contrast",
          "color": "#002033",
          "name": "Contrast (Dark)"
        }
      ],
      "gradients": [
        {
          "slug": "primary-to-secondary",
          "gradient": "linear-gradient(135deg, #002033 0%, #1a3a4a 100%)",
          "name": "Primary to Secondary"
        },
        {
          "slug": "dark-overlay",
          "gradient": "linear-gradient(180deg, rgba(0,0,0,0.6) 0%, rgba(0,0,0,0.3) 50%, rgba(0,0,0,0.7) 100%)",
          "name": "Dark Overlay"
        }
      ]
    },
    "typography": {
      "fluid": true,
      "fontFamilies": [
        {
          "fontFamily": "'Cormorant Garamond', serif",
          "slug": "heading",
          "name": "Heading",
          "fontFace": [
            {
              "fontFamily": "Cormorant Garamond",
              "fontWeight": "300",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "src": [
                "file:./assets/fonts/heading-300.woff2"
              ]
            },
            {
              "fontFamily": "Cormorant Garamond",
              "fontWeight": "400",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "src": [
                "file:./assets/fonts/heading-400.woff2"
              ]
            },
            {
              "fontFamily": "Cormorant Garamond",
              "fontWeight": "500",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "src": [
                "file:./assets/fonts/heading-500.woff2"
              ]
            },
            {
              "fontFamily": "Cormorant Garamond",
              "fontWeight": "600",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "src": [
                "file:./assets/fonts/heading-600.woff2"
              ]
            },
            {
              "fontFamily": "Cormorant Garamond",
              "fontWeight": "700",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "src": [
                "file:./assets/fonts/heading-700.woff2"
              ]
            }
          ]
        },
        {
          "fontFamily": "'DM Sans', sans-serif",
          "slug": "body",
          "name": "Body",
          "fontFace": [
            {
              "fontFamily": "DM Sans",
              "fontWeight": "300",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "src": [
                "file:./assets/fonts/body-300.woff2"
              ]
            },
            {
              "fontFamily": "DM Sans",
              "fontWeight": "400",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "src": [
                "file:./assets/fonts/body-400.woff2"
              ]
            },
            {
              "fontFamily": "DM Sans",
              "fontWeight": "500",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "src": [
                "file:./assets/fonts/body-500.woff2"
              ]
            },
            {
              "fontFamily": "DM Sans",
              "fontWeight": "600",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "src": [
                "file:./assets/fonts/body-600.woff2"
              ]
            },
            {
              "fontFamily": "DM Sans",
              "fontWeight": "700",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "src": [
                "file:./assets/fonts/body-700.woff2"
              ]
            }
          ]
        }
      ],
      "fontSizes": [
        {
          "slug": "small",
          "size": "0.875rem",
          "name": "Small",
          "fluid": false
        },
        {
          "slug": "medium",
          "size": "1rem",
          "name": "Medium",
          "fluid": false
        },
        {
          "slug": "large",
          "size": "1.25rem",
          "name": "Large",
          "fluid": {
            "min": "1.125rem",
            "max": "1.25rem"
          }
        },
        {
          "slug": "x-large",
          "size": "clamp(1.5rem, 3vw, 2rem)",
          "name": "X-Large",
          "fluid": false
        },
        {
          "slug": "xx-large",
          "size": "clamp(2rem, 4vw, 3rem)",
          "name": "XX-Large",
          "fluid": false
        },
        {
          "slug": "huge",
          "size": "clamp(2.5rem, 5vw, 4rem)",
          "name": "Huge",
          "fluid": false
        },
        {
          "slug": "gigantic",
          "size": "clamp(3rem, 8vw, 6rem)",
          "name": "Gigantic",
          "fluid": false
        }
      ]
    },
    "spacing": {
      "spacingScale": {
        "steps": 0
      },
      "spacingSizes": [
        {
          "slug": "20",
          "size": "0.5rem",
          "name": "2X-Small"
        },
        {
          "slug": "30",
          "size": "clamp(0.75rem, 1.5vw, 1rem)",
          "name": "X-Small"
        },
        {
          "slug": "40",
          "size": "clamp(1rem, 2vw, 1.5rem)",
          "name": "Small"
        },
        {
          "slug": "50",
          "size": "clamp(1.5rem, 3vw, 2rem)",
          "name": "Medium"
        },
        {
          "slug": "60",
          "size": "clamp(2rem, 4vw, 3rem)",
          "name": "Large"
        },
        {
          "slug": "70",
          "size": "clamp(3rem, 6vw, 5rem)",
          "name": "X-Large"
        },
        {
          "slug": "80",
          "size": "clamp(4rem, 8vw, 8rem)",
          "name": "2X-Large"
        }
      ],
      "units": [
        "%",
        "px",
        "em",
        "rem",
        "vh",
        "vw"
      ]
    },
    "layout": {
      "contentSize": "1200px",
      "wideSize": "1400px"
    },
    "custom": {
      "fontWeight": {
        "light": 300,
        "normal": 400,
        "medium": 500,
        "semibold": 600,
        "bold": 700
      }
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--base)",
      "text": "var(--wp--preset--color--contrast)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--body)",
      "fontSize": "var(--wp--preset--font-size--medium)",
      "lineHeight": "1.6"
    },
    "spacing": {
      "padding": {
        "top": "0",
        "right": "0",
        "bottom": "0",
        "left": "0"
      }
    },
    "elements": {
      "heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--heading)",
          "fontWeight": "400",
          "lineHeight": "1.2"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--gigantic)"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--huge)"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xx-large)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large)"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--accent)"
          }
        }
      },
      "button": {
        "color": {
          "background": "var(--wp--preset--color--accent)",
          "text": "var(--wp--preset--color--base)"
        },
        "typography": {
          "fontSize": "0.75rem",
          "fontWeight": "500",
          "textTransform": "uppercase",
          "letterSpacing": "0.15em"
        },
        "border": {
          "radius": "0"
        },
        "spacing": {
          "padding": {
            "top": "1.25rem",
            "right": "2.5rem",
            "bottom": "1.25rem",
            "left": "2.5rem"
          }
        }
      }
    },
    "blocks": {
      "core/cover": {
        "spacing": {
          "padding": {
            "top": "0",
            "right": "0",
            "bottom": "0",
            "left": "0"
          }
        }
      },
      "core/button": {
        "border": {
          "radius": "0"
        },
        "typography": {
          "fontSize": "0.75rem",
          "letterSpacing": "0.15em",
          "textTransform": "uppercase"
        }
      },
      "core/heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--heading)",
          "fontWeight": "300"
        }
      },
      "core/paragraph": {
        "typography": {
          "lineHeight": "1.7"
        }
      },
      "core/columns": {
        "spacing": {
          "blockGap": "var(--wp--preset--spacing--50)"
        }
      },
      "core/group": {
        "spacing": {
          "blockGap": "var(--wp--preset--spacing--40)"
        }
      }
    }
  }
}